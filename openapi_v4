          type: string
          format: date
          description: 'Live estimated date of delivery'
          example: '2021-04-16'
        EstimatedDeliveryTime:
          type: string
          description: 'Live estimated time of delivery (only available during day of delivery), does not include margin of error'
          example: '13:15'
