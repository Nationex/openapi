            type: string
            enum: [landscape, portrait]
            default: landscape
        - name: format
          in: query
          description: Page format of the file
          required: false
          schema:
            type: string
            enum: [4x6, 85x11]
            default: 4x6
          example: 4x6
      responses:
        '200':
          description: 'Success'
