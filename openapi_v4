          type: string
          description: 'Readable EstimatedTime in french'
          example: 'Entre 12:45 et 13:45'
        EstimatedDeliveryTimeEn:
          type: string
          description: 'Readable EstimatedTime in english'
          example: 'Between 12:45 and 13:45'
