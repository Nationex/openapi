          description: "Volume of the parcel in cubic foot. Calculated with 'Width * Height * Length) / 1728'"
    DTOParcelInfo:
      type: object
      properties:
        ParcelId:
          type: integer
          format: int64
          description: "11 digit number to identify the parcel"
          example: 50133944801
